{
  "name": "jameslytle-com-react",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --host 0.0.0.0",
    "build:content": "node scripts/build-content.mjs",
    "import:projects": "node scripts/import-legacy-projects.mjs",
    "import:albums": "node scripts/import-legacy-albums.mjs",
    "build:client": "vite build",
    "build:ssr": "vite build --ssr src/entry-server.jsx --outDir dist/server",
    "prerender": "node scripts/prerender.mjs",
    "build": "npm run build:content && npm run build:client && npm run build:ssr && npm run prerender",
    "a11y:check": "npm run build && node scripts/a11y-audit.mjs",
    "preview": "vite preview"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.1"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.4.1",
    "fast-glob": "^3.3.3",
    "gray-matter": "^4.0.3",
    "remark": "^15.0.1",
    "remark-html": "^16.0.1",
    "vite": "^5.4.14"
  }
}
